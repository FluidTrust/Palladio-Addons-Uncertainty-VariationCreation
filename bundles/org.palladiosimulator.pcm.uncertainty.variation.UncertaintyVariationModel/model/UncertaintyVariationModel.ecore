    <eStructuralFeatures xsi:type="ecore:EReference" name="allocationContext" lowerBound="1"
        eType="ecore:EClass ../../org.palladiosimulator.pcm/model/pcm.ecore#//allocation/AllocationContext"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="VaryingBranch" eSuperTypes="#//VariationPoint">
    <eStructuralFeatures xsi:type="ecore:EReference" name="branchActions" upperBound="-1"
        eType="ecore:EClass ../../org.palladiosimulator.pcm/model/pcm.ecore#//seff/BranchAction"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="branches" upperBound="-1"
        eType="ecore:EClass ../../org.palladiosimulator.pcm/model/pcm.ecore#//usagemodel/Branch"/>
  </eClassifiers>
